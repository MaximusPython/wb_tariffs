{
    "name": "ls_wb_tariffs",
    "version": "1.0.0",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "migrate": "ts-node -O '{\"module\":\"commonjs\"}' ./node_modules/knex/bin/cli.js migrate:latest --knexfile src/db/knexfile.ts",
        "migrate:rollback": "ts-node -O '{\"module\":\"commonjs\"}' ./node_modules/knex/bin/cli.js migrate:rollback --knexfile src/db/knexfile.ts"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "cron": "^2.3.1",
        "dotenv": "^16.0.0",
        "express": "^4.21.2",
        "googleapis": "^121.0.0",
        "knex": "^2.5.1",
        "module-alias": "^2.2.3",
        "pg": "^8.11.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.3",
        "@types/node": "^24.3.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.5.0"
    }, 
     "_moduleAliases": {
    "#app.js": "dist/app.js"
}
}